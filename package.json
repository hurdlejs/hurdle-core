{
  "name": "hurdle-core",
  "version": "1.0.0",
  "description": "Core library for hurdle",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -c .eslintrc.json src/**",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hurdlejs/hurdle-core.git"
  },
  "engines": {
    "node": ">=12.9.1"
  },
  "keywords": [
    "test",
    "core",
    "framework",
    "e2e"
  ],
  "author": "peanut jdelf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hurdlejs/hurdle-core/issues"
  },
  "homepage": "https://github.com/hurdlejs/hurdle-core#readme",
  "devDependencies": {
    "@types/got": "^9.6.7",
    "@types/graceful-fs": "^4.1.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "dependencies": {
    "got": "^9.6.0",
    "graceful-fs": "^4.2.2",
    "json-merger": "^1.1.1",
    "jsonc-parser": "^2.1.1"
  }
}
